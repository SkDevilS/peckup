# Peckup Frontend Environment Configuration
# Copy this file to .env.local and modify the values as needed

# API Configuration
VITE_API_BASE_URL=http://localhost:5000
VITE_API_PREFIX=/api

# Environment
VITE_APP_ENV=development

# Optional: Override specific endpoints
# VITE_ADMIN_URL=http://localhost:5000/admin
# VITE_UPLOAD_URL=http://localhost:5000/uploads

# Development Settings
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# Production Settings (uncomment for production)
# VITE_API_BASE_URL=https://your-production-domain.com
# VITE_APP_ENV=production
# VITE_DEBUG_MODE=false
# VITE_LOG_LEVEL=error